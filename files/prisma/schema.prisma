generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  username String  @unique
  password String
  downloads Download[]
}

model Download {
  id        String  @id @default(uuid())
  filename  String
  filepath  String
  userId    String
  user      User @relation(fields: [userId], references: [id])
}
